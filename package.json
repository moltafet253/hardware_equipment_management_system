{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm-run-all build-app && copy-files",
        "build-app": "vite build",
        "copy-files": "mkdir -p public/plugins/assets/select2 && find node_modules/select2/ -type f -exec cp {} public/plugins/assets/select2/ \\;",
        "build:prod": "cross-env NODE_ENV=production vite build"
    },
    "devDependencies": {
        "axios": "^1.1.2",
        "laravel-vite-plugin": "^0.7.5",
        "select2": "^4.1.0-rc.0",
        "tailwindcss": "^3.3.2",
        "vite": "^4.0.0"
    },
    "dependencies": {
        "@flaticon/flaticon-uicons": "^2.3.0",
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "daisyui": "^3.4.0",
        "feather-icons": "^4.29.0",
        "jquery": "^3.7.1",
        "jquery-ui-dist": "^1.13.2",
        "lodash": "^4.17.21",
        "sweetalert2": "^11.7.31"
    }
}
